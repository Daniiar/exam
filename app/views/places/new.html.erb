<h1>New Place</h1>
<%= form_for @place, html: { multipart: true } do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.label :category %>
  <%= f.collection_select :category_id, Category.all, :id , :title, prompt: true  %><br>
  <%= f.fields_for :images do |ff| %>
    <%= ff.label :image %>
    <%= ff.file_field :photo%><br><br>
  <% end %>
  <%= f.submit %>
<% end %>